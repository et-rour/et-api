  "dependencies": {
    "@types/draft-js": "^0.11.9",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.18",
    "@types/pdfkit": "^0.12.6",
    "axios": "^0.27.2",
    "body-parser": "^1.19.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "draft-js": "^0.11.7",
    "express": "^4.17.2",
    "firebase": "^9.6.7",
    "firebase-admin": "^10.0.2",
    "gpt3-tokenizer": "^1.1.5",
    "heic-convert": "^1.2.4",
    "helmet": "^5.0.2",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
